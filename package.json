{
  "name": "sport-wizard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sportwizard.se",
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.1.5",
    "@supabase/supabase-js": "^2.90.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "ajv": "8.17.1",
    "gh-pages": "^6.3.0",
    "i18next": "25.8.4",
    "lucide-react": "^0.563.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-i18next": "16.5.4",
    "react-scripts": "5.0.1",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node scripts/runTestsFailOnly.js",
    "test:performance": "RUN_PERFORMANCE_TESTS=true node scripts/runTestsFailOnly.js --watchAll=false",
    "test:performance:watch": "RUN_PERFORMANCE_TESTS=true node scripts/runTestsFailOnly.js",
    "eject": "react-scripts eject",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:restart": "supabase stop && supabase start",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push --local",
    "db:seed": "supabase db reset --db-url postgresql://postgres:postgres@127.0.0.1:54322/postgres",
    "db:diff": "supabase db diff",
    "db:types": "supabase gen types typescript --local > src/types/supabase.ts",
    "db:pull": "supabase db pull",
    "db:link": "supabase link",
    "db:unlink": "supabase unlink"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.3",
    "supabase": "2.76.4",
    "tailwindcss": "^3.4.17"
  }
}
