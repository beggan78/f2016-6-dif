{
  "name": "sport-wizard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sportwizard.se",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.31",
    "@supabase/supabase-js": "^2.51.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "gh-pages": "^6.3.0",
    "lucide-react": "^0.511.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node scripts/runTestsFailOnly.js",
    "test:performance": "RUN_PERFORMANCE_TESTS=true node scripts/runTestsFailOnly.js --watchAll=false",
    "test:performance:watch": "RUN_PERFORMANCE_TESTS=true node scripts/runTestsFailOnly.js",
    "eject": "react-scripts eject",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:restart": "supabase stop && supabase start",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push --local",
    "db:seed": "supabase db reset --db-url postgresql://postgres:postgres@127.0.0.1:54322/postgres",
    "db:diff": "supabase db diff",
    "db:types": "supabase gen types typescript --local > src/types/supabase.ts",
    "db:pull": "supabase db pull",
    "db:link": "supabase link",
    "db:unlink": "supabase unlink"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "supabase": "2.31.4",
    "tailwindcss": "^3.4.17"
  }
}
